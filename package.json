{
  "name": "stack_node_v2",
  "version": "1.0.0",
  "description": "stack node v2",
  "main": "index.js",
  "scripts": {
    "start": "node dist/main.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec ts-node src/main.ts",
    "dev:local": "cross-env NODE_ENV=local nodemon src/main.ts",
    "dev:development": "cross-env NODE_ENV=development nodemon src/main.ts",
    "dev:production": "cross-env NODE_ENV=production nodemon src/main.ts",
    "production": "cross-env NODE_ENV=production node dist/main.js",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint \"src/**/*.ts\"",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- migration:generate -d src/infrastructure/database/data-source.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/infrastructure/database/data-source.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/infrastructure/database/data-source.ts",
    "db:seed": "ts-node src/infrastructure/database/seeds/index.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "generate-key": "node generate-key.js",
    "generate-key:1d": "node generate-key.js 1d",
    "generate-key:7d": "node generate-key.js 7d"
  },
  "author": "Juan Vasquez",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "i18next": "^23.10.0",
    "i18next-fs-backend": "^2.3.1",
    "i18next-http-middleware": "^3.5.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "mysql2": "^3.14.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.1",
    "nodemailer-express-handlebars": "^6.1.0",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.8.1",
    "typeorm": "^0.3.21",
    "uuid": "^11.1.0",
    "winston": "^3.12.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/handlebars": "^4.0.40",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/ms": "^2.1.0",
    "@types/node": "^22.13.14",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/nodemailer-express-handlebars": "^4.0.5",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
